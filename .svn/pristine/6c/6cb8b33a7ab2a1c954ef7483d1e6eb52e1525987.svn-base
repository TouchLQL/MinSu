        #demospace{
			height:100%;
			padding-top:300px;
			-webkit-perspective:800;
			-moz-perspective:800px;
			perspective:800px;
			-webkit-perspective-origin:50% 50%;
			-moz-perspective-origin:50% 50%;
			perspective-origin:50% 50%;
			background: -webkit-radial-gradient(center, ellipse, #430d6d 0%, #000000 100%);
		}
		#box{
			height:200px;
			width:200px;
			margin:100px auto;
			-moz-transform-style:-moz-preserve-3d;
			-webkit-transform-style:preserve-3d;
			transform-style:preserve-3d;
			position:relative;
			-webkit-animation: 6s spin linear infinite;
		}
		.box{
			height:200px;
			width:200px;
			border:1px solid #000;
			color:#000;
			font-size:40px;
			line-height:200px;
			text-align:center;
			background:#240048;
			color:#000;
			opacity:0.6;
			position:absolute;
			}
			.text2{
				display:inline-block;
				width:100%;
			}
	#box_front{
		-webkit-transform-origin:bottom;
	
		}
		#box_left{
			-webkit-transform-origin:left;
			-webkit-transform:rotateY(90deg);
		
			}
		#box_right{
			-webkit-transform-origin:right;
			-webkit-transform:rotateY(-90deg);
			
			}
		#box_behind{
			-webkit-transform:translateZ(-200px);
			
			}
		#box_bottom{
			-webkit-transform-origin:bottom;
			-webkit-transform:rotateX(90deg);
			
			}
		#box_top{
			-webkit-transform-origin:bottom;
			-webkit-transform:rotateX(90deg) translateZ(200px);
			}
			.l_btn{
				background:#FF6633;
				color:#fff;
				opacity:1;
			}
  @-webkit-keyframes spin{
	  	from
	  	{
			-webkit-transform:rotateX(20deg) rotateY(45deg) rotateZ(0deg);
		  
		  }
		to
		{
			-webkit-transform:rotateX(380deg) rotateY(405deg) rotateZ(0deg);
			}
	  
	  }